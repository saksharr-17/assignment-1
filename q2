#include <stdio.h>

int main() {
    int i,j,a,b,hcf = 1;
    printf("Enter first number");
    scanf("%d",&a);
    printf("Enter second number");
    scanf("%d",&b);

    j = (a < b) ? a : b;
    
   for(i = 1; i <= j; i++)  
    {  
        if(a%i == 0 && b%i == 0)  
        {  
            hcf = i; 
        }  
    }  

   printf("The hcf is %d",hcf);  
    return 0;
}
